   ],
   "source": [
    "a = 18.5\n",
    "d = 2.5\n",
    "n = 250\n",
    "print(f\"250 ∑ i=1 (18.5 + 2.5i)\\n>> {n/2*(2*a+(n-1)*d)} o aproximado {n/2*(a+a+(n-1)*d):.0f}\") "
